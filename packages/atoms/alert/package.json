{
  "name": "@payments-component-library-test/alert",
  "version": "1.1.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "build:esm": "tsc --outDir lib/esm",
    "build:d": "tsc --declaration true --emitDeclarationOnly true --declarationDir lib/types --isolatedModules false --sourceMap false",
    "build": "npm run build:esm && npm run build:cjs && npm run build:d",
    "clean": "rm -r ./lib ||: && rm -r ./.nyc_output ||: && rm -r ./coverage ||:",
    "lint": "eslint ./{src,__tests__} --ext .ts,.tsx",
    "cy:open": "cypress open-ct --config-file ../cypress.json",
    "cy:unit-headless": "cypress run-ct --browser chrome --config-file ../cypress-atoms-base.json",
    "coverage-gate": "nyc check-coverage --lines 100 --statements 100 --branches 100 --functions 100",
    "test": "jest --coverage",
    "abc": "node ./Alert.tsx"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@bahmutov/cypress-extends": "1.1.0",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/react": "^5.10.2",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@styled-system/css": "^5.1.5",
    "@styled-system/prop-types": "^5.1.5",
    "@testing-library/cypress": "^7.0.5",
    "@types/jest": "^27.5.0",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^5.1.5",
    "@types/styled-system__css": "5.0.14",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^28.0.3",
    "babel-plugin-styled-components": "^1.11.1",
    "chai-colors": "^1.0.1",
    "cypress": "^9.5.4",
    "eslint": "7.32.0",
    "eslint-plugin-cypress": "^2.11.2",
